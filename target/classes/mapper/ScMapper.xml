<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cms.mapper.ScMapper">

    <select id="selectScList" resultMap="Sc">
        select Sname,Cname,Grader
        from student,sc,course
        where sc.Sno = student.Sno
    </select>

    <select id="getCname" resultType="Course">
        select Cname from course,sc where sc.Cno=course.Cno
    </select>

</mapper>
