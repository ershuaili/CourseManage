<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cms.mapper.TeacherMapper">
    <!--根据教师编号名查询教师-->
    <select id="selectTea" resultType="Teacher">
        select *
        from teacher
        where Tno = #{teaNo}
    </select>

</mapper>
